{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    { "src": "/images/(.*)", "dest": "/public/images/$1" },
    { "src": "/javascripts/(.*)", "dest": "/public/javascripts/$1" },
    { "src": "/stylesheets/(.*)", "dest": "/public/stylesheets/$1" },
    { "src": "/(.*)", "dest": "/app.js" }
  ]
}
